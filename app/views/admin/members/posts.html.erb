<div class="container">
  <div class="row">
    <div class="col-md-8 bg-white mb-3 rounded py-3">
      <div class="d-flex">
        <h2><%= @member.name %>さんの記事一覧</h2>
      </div>
      <%= render 'admin/posts/index', posts: @posts %>
      <%= paginate @posts %>
    </div>
    <div class="col-md-4">
      <%= render 'admin/posts/tag', tags: @tags, favorited_tags: @favorited_tags %>
    </div>
  </div>
</div>